	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Package - no params",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}",
		},
		{
			"name": "Launch Package - rm **",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}",
			"args": ["rm", "**"]
		},
		{
			"name": "Launch Package - ls",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}",
			"args": ["ls"]
		},
		{
			"name": "Launch Package - add test",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}",
			"args": ["add", "test"]
		},
	]
}